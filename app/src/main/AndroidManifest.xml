<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mymusic.music">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.Activity.Login.LoginActivity" />
        <activity android:name=".View.Activity.Login.LoginPhoneActivity" />
        <activity android:name=".View.Activity.Login.RegisterActivity" />
        <activity android:name=".View.Activity.Login.ForgetPwdActivity" />
        <activity android:name=".View.Activity.user.UserActivity" />
        <activity android:name=".View.Activity.Detail.DetailsActivity" />
        <activity android:name=".View.Activity.MyActivity.MywalletActivity" />
        <activity android:name=".View.Activity.MyActivity.MyLiveActivity"></activity>
    </application>

</manifest>